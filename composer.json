{
    "name": "DejwCake/ExtendedAuthenticate",
    "description": "DejwCake/ExtendedAuthenticate plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.6.*",
        "cakephp/cakephp": ">=3.4.* <4.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "DejwCake\\ExtendedAuthenticate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DejwCake\\ExtendedAuthenticate\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
